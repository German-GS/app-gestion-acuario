{
  "expo": {
    "name": "AquaMind",
    "slug": "AquaMind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icono.png",
    "scheme": "aquamind",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aquamind.app",
      "jsEngine": "hermes",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.aquamind.app",
      "jsEngine": "hermes",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icono.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 2
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1b186bbe-733f-45c7-b8b3-9bf34933413d"
      }
    }
  }
}